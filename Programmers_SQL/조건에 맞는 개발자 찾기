
SELECT  DEV.ID,
        DEV.EMAIL,
        DEV.FIRST_NAME,
        DEV.LAST_NAME
FROM    DEVELOPERS AS DEV
## 비트 연산을 통해 해당 스킬을 보유하고있는지 확인한다.
WHERE   (DEV.SKILL_CODE & (SELECT CODE FROM SKILLCODES WHERE NAME = 'C#')) 
        OR
        (DEV.SKILL_CODE & (SELECT CODE FROM SKILLCODES WHERE NAME = 'Python')) 
ORDER BY DEV.ID ASC
